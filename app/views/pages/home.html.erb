<%= render partial: 'shared/most_popular_tags' %>
<%= render partial: 'shared/search_panel' %>

<div class='container-fluid main_page_container'>

    <div class='row main_page_row'>

        <div class='main_page_title'>
            <h2>Welcome on PhotoClicker.net - Free Stock Photos Page</h2>
            <h3>Our goal is to provide people, bloggers, designers, graphic designers and many other professionals,<br> access to absolutely free, high-quality photos provided by photographers from all over the world.</h3>
        </div>

    
        <div class='col-md-12 main_page_isotope_column'>

             <%= render 'photos' %>

        </div>

        <div class="page-load-status">
            <div class="loader-ellips infinite-scroll-request">
                <span class="loader-ellips__dot"></span>
                <span class="loader-ellips__dot"></span>
                <span class="loader-ellips__dot"></span>
                <span class="loader-ellips__dot"></span>
            </div>
            <p class="infinite-scroll-last">End of content</p>
            <p class="infinite-scroll-error">No more pages to load</p>
        </div>


		<div class='pagination'>

			<%= will_paginate @photos %>

		</div>


    </div>

</div>